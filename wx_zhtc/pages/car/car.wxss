page {
  background: rgba(244, 244, 244, 1);
  margin-top: 20rpx;
  font-size: 26rpx;
  padding-bottom: 110rpx;
}

/* 最外层框架修饰 */

view.body {
  width: 95%;
  margin: 0 auto;
}

/* 修饰票类狂 */

view.tick_type {
  background: #fff;
  border-radius: 10rpx 10rpx;
}

view.tick_type_name {
  border-bottom: 1rpx solid rgba(235, 235, 235, 1);
  position: relative;
  padding-top: 26rpx;
  padding-bottom: 26rpx;
}

view.ned_konw {
  position: absolute;
  width: 150rpx;
  top: 70rpx;
  right: 30rpx;
}

text.name {
  display: block;
  font-weight: bold;
  margin: 10rpx 0rpx;
  color: rgba(89, 89, 89, 1);
  font-size: 30rpx;
}

view.ned_konw>image {
  height: 26rpx;
  width: 26rpx;
  margin-left: 10rpx;
}

view.ned_konw>text {
  color: #1f90f9;
}

view.xianshi {
  width: 95%;
  margin: 0 auto;
}

/* 使用日期 */

view.date {
  background: #fff;
  margin-top: 20rpx;
  border-radius: 10rpx 10rpx;
  padding: 20rpx 0rpx;
}

view.date_body {
  width: 95%;
  margin: 0 auto;
  overflow: hidden;
}

view.date_body>text {
  display: block;
  font-weight: 500;
  color: rgba(39, 39, 39, 1);
  font-size: 30rpx;
  margin-bottom: 20rpx;
}

view.date_body_son {
  float: left;
  width: 23%;
  box-sizing: border-box;
  padding: 0rpx 10rpx;
  height: 100rpx;
  border: 1rpx solid #1f90f9;
  border-radius: 10rpx 10rpx;
  position: relative;
}

/* 选中的问题 */

image.isSelect {
  position: absolute;
  height: 44rpx;
  width: 50rpx;
  right: 0rpx;
  bottom: 0rpx;
}

/* image.isSelect_0{
 display:block;
height:45rpx  ;
width:50rpx ;
position:absolute ;
top:50rpx ;
right:-2rpx  ;

} */

view.date_body_son>text {
  display: block;
}

view.date_body_son>text:first-child {
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

view.tomorrow {
  margin: 0 2%;
}

view.afterTomorrow {
  margin-right: 2%;
}

view.more {
  position: relative;
  padding-top: 12rpx;
}

view.more text {
  display: block;
  text-align: center;
}

image.dayu {
  display: block;
  height: 45rpx;
  width: 25rpx;
  position: absolute;
  top: 40rpx;
  right: 3rpx;
}

text.money {
  color: red;
  margin-top: 10rpx;
  text-align: center;
}

text.time {
  color: #1f90f9;
}

/* 票类 */

view.wai {
  border-bottom: 1rpx solid rgba(235, 235, 235, 1);
  padding: 46rpx 0rpx;
}

view.zhonglei {
  margin-top: 20rpx;
  background: #fff;
  border-radius: 10rpx 10rpx;
}

view.zhonglei_body {
  width: 95%;
  height: 120rpx;
  margin: 0 auto;
}

view.zhonglei_body_one {
  margin-bottom: 40rpx;
  overflow: hidden;
}

view.zhonglei_body_one>view {
  float: left;
  width: 70%;
  font-size: 36rpx;
  font-weight: 500;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

view.zhonglei_body_one>view:last-child {
  text-align: right;
  width: 30%;
  color: red;
}

view.zhonglei_body_two {
  position: relative;
}

view.zhonglei_body_two>view:first-child {
  color: #1f90f9;
}

image.needkonw {
  height: 30rpx;
  display: block;
  position: absolute;
  top: 5rpx;
  left: 110rpx;
  width: 30rpx;
}
view.jisuan {
  position: absolute;
  overflow: hidden;
  /* background: red; */
  width: 240rpx;
  top: -10rpx;
  right: 2rpx;
  border: 1px solid rgba(229, 229, 229, 1);
}
view.jisuan>view {
  float: left;
  width: 33.3%;
  box-sizing: border-box;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
}

view.jian {
  font-size: 60rpx;
  line-height: 50rpx !important;
}

view.add {
  font-size: 60rpx;
  line-height: 50rpx !important;
  color: #1f90f9;
}

view.jisuan>view:nth-child(2) {
  border-right: 2px solid rgba(229, 229, 229, 1);
  border-left: 2px solid rgba(229, 229, 229, 1);
}

/* 填写信息 */

view.informations {
  margin-top: 20rpx;
  background: #fff;
  border-radius: 10rpx 10rpx;
}

view.informations_body {
  width: 95%;
  margin: 0 auto;
  padding: 20rpx 0rpx;
}

text.tips {
  font-size: 24rpx;
  font-weight: bold;
}

text.people {
  color: red;
}

text.needwrite {
  color: rgba(149, 149, 149, 1);
}

/* 表单 */

view.form {
  margin-top: 50rpx;
}

view.input {
  overflow: hidden;
  border-top: 1rpx solid rgba(235, 235, 235, 0.5);
}

view.input>view {
  float: left;
  width: 20%;
  height: 80rpx;
  line-height: 80rpx;
}

view.input>view:nth-child(2) {
  width: 50%;
  margin-left: 5%;
}

view.input>view:nth-child(2)>input {
  height: 100%;
}

view.input>view:nth-child(3) {
  width: 10%;
  /* margin-left: 5%; */
}

view.input>view:last-child>input {
  height: 100%;
  line-height: 80rpx;
}

view.inputphone {
  position: relative;
}

button.queryphone {
  font-size: 18rpx !important;
  height: 50rpx;
  color: grey;
  width: 150rpx;
  margin-top: 17rpx;
  line-height: 50rpx;
  font-weight: bold;
  position: absolute;
  right: 0;
  display: block;
}

/* 底部 */

view.bottom_bar {
  width: 100%;
  height: 100rpx;
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  background: #fff;
  z-index: 10;
}

view.bottom_bar>view {
  float: left;
  width: 50%;
  height: 100%;
  text-align: center;
  line-height: 100rpx;
}

view.go_buy {
  background: linear-gradient(90deg, rgba(85, 164, 225, 1) 0%, rgba(63, 106, 223, 1) 100%);
  font-size: 36rpx;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
}

view.cunt {
  font-size: 30rpx;
  font-weight: bold;
  color: red;
}

/**时间选择框架***/

view.timeSelectBox {
  width: 100%;
  position: fixed;
  bottom: 0;
  height: 100%;
  font-size: 32rpx;
  background: rgba(0, 0, 0, 0.55);
  z-index: 11;
  font-weight: 900;
}

view.canlendarBgView {
  background: #fff;
  position: fixed;
  width: 100%;
  bottom: 0;
  height: 60%;
  padding-bottom: 140rpx;
  padding-top: 34rpx;
  border-top: 1rpx solid gray;
}

view.canlendarTopView {
  display: flex;
  text-align: center;
}

view.canlendarTopView>view {
  width: 33.3%;
}

/**工作日*/

view.weekBgView {
  display: flex;
  margin-top: 25rpx;
  margin-bottom: 25rpx;
}

view.weekBgView>view {
  width: 14.2%;
  /* height: 100rpx; */
  text-align: center;
}

view.dateBgView {
  overflow: hidden;
}

view.dateBgView>view {
  width: 14.2%;
  float: left;
  /* height: 80rpx; */
  line-height: 100rpx;
  text-align: center;
  margin-bottom: 4rpx;
}

.dateSelectView {
  /* background: red; */
  /* color: #fff; */
  color: red;
  /* border-radius: 50% 50%; */
}
/**重构**/
.dateView{
  position: relative;
}

/*确定取消按钮*/

view.clickButton {
  height: 60rpx;
  /* background: blue; */
  margin-bottom: 20rpx;
  overflow: hidden;
  position: relative;
}

view.clickButton>text {
  display: block;
  height: 60rpx;
  width: 100rpx;
  border-radius: 10rpx 10rpx;
  color: red;
  text-align: center;
  line-height: 60rpx;
  position: absolute;
  bottom: 0;
  /* left: 20rpx; */
}

text.ok {
  right: 20px !important;
  color: #1f90f9 !important;
  /* background: blue !important; */
}

text.no {
  left: 20px !important;
  color: red !important;
  /* background: blue !important; */
}

.dateSelectView_o {
  color: lightgray !important;
}

view.chosedate {
  position: absolute;
  left: 50%;
  display: block;
  width: 200rpx;
  text-align: center;
  bottom: 0;
  margin-left: -100rpx;
  height: 60rpx;
  line-height: 60rpx;
}
