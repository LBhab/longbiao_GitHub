page {
  background: rgba(246, 246, 246, 1);
  font-size: 28rpx;
  padding-bottom: 100rpx;
}

/* logo修饰 */

view.logo {
  background: #fff;
  position: relative;
}

text.adress_name {
  font-size: 36rpx;
  font-family: PingFang-SC-Bold;
  font-weight: bold;
  color: rgba(254, 254, 254, 1);
  display: block;
  position: absolute;
  top: 256rpx;
  left: 28rpx;
  z-index: 22;
}

.slide-image {
  display: block;
  height: 100%;
  width: 100%;
}

.lunbo {
  width: 100%;
  height: 380rpx;
}

view.logo_s {
  margin: 0 aoto;
  padding-left: 40rpx;
  position: relative;
}

view.discrible {
  border-bottom: 1rpx solid rgba(235, 235, 235, 1);
  height: 100rpx;
  padding-bottom: 20rpx;
}

view.discrible>text {
  display: block;
}

view.discrible>text:first-child {
  font-weight: 600;
  color: rgba(73, 73, 73, 1);
  font-size: 30rpx;
  margin: 20rpx 0rpx;
}

view.discrible>text.time {
  color: rgba(181, 181, 181, 1);
  margin: 20rpx 0rpx;
}

view.logo_s>image.jinru {
  display: block;
  position: absolute;
  height: 30rpx;
  width: 30rpx;
  right: 20rpx;
  top: 50%;
  margin-top: -15rpx;
}

view.logo_s>image.weizhi {
  display: block;
  position: absolute;
  height: 34rpx;
  width: 34rpx;
  left: 36rpx;
  top: 50%;
  margin-top: -17rpx;
}

view.address {
  line-height: 40rpx;
  margin-bottom: 20rpx;
}

view.address>text {
  font-weight: 600;
  color: rgba(73, 73, 73, 1);
  font-size: 24rpx;
  margin-left: 40rpx;
  line-height: 100rpx;
}

/* 常用票种 */

view.tekick {
  background: #fff;
  margin-bottom: 20rpx;
}

view.always_tekick {
  width: 90%;
  margin: 0 auto;
}

text.title {
  font-weight: 600;
  color: rgba(73, 73, 73, 1);
  font-size: 30rpx;
  padding: 20rpx 0rpx;
  display: block;
}

view.xiangq {
  border-top: 1rpx solid rgba(235, 235, 235, 1);
  position: relative;
  padding: 37rpx 0rpx;
}

/* 修饰 */

view.main {
  overflow: hidden;
}

view.main>view {
  float: left;
  width: 10%;
}

view.main>view:last-child {
  width: 90%;
}

.boxInput {
  margin-top: 50rpx;
}

/* 重写复选框样式 */

